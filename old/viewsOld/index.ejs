    <body>
        
        <!--===sign up form starts here===-->
        
        	<div class="sign-up-wrapper login-container v-center">
            	<form action="/initAccount" method="post">
                	<h4>REGISTRATION<span data-dismiss="modal"><img src="images/cross.png" alt="image" title="cross-icon"/></span></h4>
                  <div class="form-group">
                    <label >Name</label>
                    <input type="text" name="username" id="username" class="form-control" required>
                  </div>
                  
                 <div class="form-group">
                    <label >Password</label>
                    <input type="password" name="password" id="password" class="form-control" required>
                  </div>
                  <div class="form-group">
                    <label >Confirm Password</label>
                    <input type="password" name="confirmPassword" id="confirmPassword" class="form-control" required>
                  </div>
                  
                  <div class="row form-group">
                                        <div class="col-sm-4"><label>Street/Address :</label></div>
                                        <div class="col-sm-8"><input type="text" placeholder="Enter Address" class="form-control" name="address1" id="address1" required></div>
                                    </div>
                                     <div class="row form-group">
                                        <div class="col-sm-4"><label>Country :</label></div>
                                        <div class="col-sm-8">
                                        	<select class="form-control" name="address4" id="address4" required>
                                            	<option value="">Select Country</option>
                                                <option>India</option>
                                                <option>Pakistan</option>
                                                <option>UK</option>
                                                <option>Japan</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="row form-group">
                                        <div class="col-sm-4"><label>State :</label></div>
                                        <div class="col-sm-8">
                                        	<select class="form-control" name="address3" id="address3" required>
                                            	<option value="">Select State</option>
                                                <option>Delhi</option>
                                                <option>Up</option>
                                                <option>Jhanrkhand</option>
                                                <option>Mumbai</option>
                                            </select>
                                        </div>
                                    </div>
                                     <div class="row form-group">
                                        <div class="col-sm-4"><label>City:</label></div>
                                        <div class="col-sm-8">
                                        	<select class="form-control" name="address2" id="address2" required>
                                            	<option value="">Select City</option>
                                                <option>Delhi</option>
                                                <option>Mumbai</option>
                                                <option>Bangoluro</option>
                                                <option>Mumbai</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="row form-group">
                                        
                                        <div class="col-sm-8">
                                        	<select class="form-control" name="type" id="type" required>
                                                <option value="">Select Type</option>
                                            	<option value="subdistributor">Sub-Distributor</option>
                                                <option value="wholeseller">Wholeseller</option>
                                                <option value="retailer">Retailer</option>
                                                
                                            </select>
                                        </div>
                                    </div>

                  <div class="form-group">
                    <button type="submit" class="btn btn-primary center-block">Sign Up</button>
                  </div>
                  <div class="form-group">
                    <a href="/login" >Already have an account?</a>
                  </div>
                </form>

            </div>
        
        <!--===sign up form ends here=====-->
        
        
        
