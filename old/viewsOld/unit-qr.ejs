        =====================================================================-->
        <style>
            .canvasParent{
                width: 130px !important;
                margin: auto;
            }
            .canvasParent svg{ width: 100%;}
        </style>
    </head>
    <body class="hold-transition sidebar-mini">
        <!-- Site wrapper -->
        <div class="wrapper">
            <header class="main-header"> 
                <a href="#" class="logo"> <!-- Logo -->
                    <span class="logo-mini">
                        <!--<b>A</b>BD-->
                        <img src="/assets/dist/img/mini-logo.png" alt="">
                    </span>
                    <span class="logo-lg">
                        <!--<b>Admin</b>BD-->
                        <img src="/assets/dist/img/logo.png" alt="">
                    </span>
                </a>
                <!-- Header Navbar -->
                <nav class="navbar navbar-static-top">
                    <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button"> <!-- Sidebar toggle button-->
                        <span class="sr-only">Toggle navigation</span>
                        <span class="pe-7s-keypad"></span>
                    </a>
                    <div class="navbar-custom-menu">
                        <ul class="nav navbar-nav">
                            <li class="dropdown dropdown-user">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown"> <i class="fa fa-user-circle-o v-middle" aria-hidden="true"></i>Profile</a>
                                <ul class="dropdown-menu">
                                   <!-- <li><a href="profile.html"><i class="pe-7s-users"></i> User Profile</a></li>
                                    <li><a href="#"><i class="pe-7s-settings"></i> Settings</a></li>-->
                                    <li><a href="/logout"><i class="pe-7s-key"></i> Logout</a></li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                </nav>
            </header>
            <!-- =============================================== -->
            <!-- Left side column. contains the sidebar -->
            
                <% include partials/oem-navigation %>
            
            
            <!-- =============================================== -->
            <!-- Content Wrapper. Contains page content -->
            <div class="content-wrapper">
                <!-- Main content -->
                <section class="content" style="margin-top:15px;">
                    <div class="row">
                         <!-- Modal -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">View History</h4>
      </div>
      
     <!-- <div class="modal-footer">
        <button type="button" class="btn btn-primary">View Sub Unit</button>
      </div>-->
    </div>
  </div>
</div>

                        <!-- Textual inputs -->
                        <div class="col-sm-12">
                            <div class="panel panel-bd unit-maneg-page">
                                <div class="panel-heading clearfix">
                                   <div class="panel-title pull-left">
                                        <h2>Unit Smartcode</h2>
                                        
                                    </div>
                                    <div class="col-sm-12">
                                        <div class="panel panel-bd unit-maneg-page">
                                        <div class="row-sm-12">

                                <table class="table table-bordered">
                                    
                                    <div class="panel panel-bd unit-maneg-page">
                                        <div class="header-title">
                                        <h3>Details-</h3>
                                        </div>
                                    </div>
                                    
                                    <tbody>
                            
                              <tr class="active">
                                <td align="center"><b>Batch No.-</td>
                                <td align="center"><%=BatchNo%></td>
                              </tr>
                              <tr class="active">
                                <td align="center"><b>SKU Id-</td>
                                <td align="center"><%=skuId%></td>
                              </tr>
                              <tr class="active">
                                <td align="center"><b>Manufactured Date-</td>
                                <td align="center"><%=ManufactureDate%></td>
                              </tr>
                              <tr class="active">
                                <td align="center"><b>Expiry Date-</td>
                                <td align="center"><%=ExpiryDate%></td>
                              </tr>
                              
                             
                             
                                    </tbody>
                                    

                                </table>
                                                    <input id="count" value="<%=keys.length%>" type="hidden"> </input>

                                                    <% for (var i=0;i<keys.length;i++){%>
                                                        <input id="field<%=i%>" value="<%=keys[i]%>" type="hidden" role="document"> </input>
                                                    <%}%>
                                                    <div id="print-content">
                                                    <table class="table table-bordered">
                                    
                                                      <div class="panel panel-bd unit-maneg-page">
                                                          <div class="header-title">
                                                          <h3>Smartcodes-</h3>
                                                          </div>
                                                      </div>
                                                      <thead>
                            <tr>
                              <th width="200" class="text-center">Smartcode Address</th>
                              <th width="150" class="text-center">QR Code</th>
                             
                             
                            </tr>
                          </thead>
                                                      <tbody>
                                              
                                                
                                                  <% for (var i=0;i<keys.length;i++){%>
                                                    <tr class="active">
                                                  <td>
                                                    <%=addresses[i]%>
                                                  </td>
                                                    
                                                  <td>
                                                        <div id="response<%=i%>">
                                                            <canvas id="canvas"></canvas>
                                                        </div>
                                                        </td>
                                                </tr>
                                                        <%}%>
                                                  
                                                      </tbody>
                                                      

                                                  </table>
                                                    
                                                        
                                                    </div>
                                                    <input type="button" onclick="printDiv('print-content')" value="Print"/>
                                                </div>    

                                                        </div>
                                                    </div>
                                                    
                                    </div>
                                                    
                                                    
                                
                                
                                
                            </div>
                        </div>
                        
                    </div>
                    <!-- Checkboxes & Radios -->
                </section> <!-- /.content -->
            </div> <!-- /.content-wrapper -->
            <footer class="main-footer">
                <div class="pull-right hidden-xs"> <b>Version</b> 1.0</div>
