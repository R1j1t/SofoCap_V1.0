                <!-- Main content -->
                
                   
                        

                        <!-- Textual inputs -->
                        <div class="col-sm-12">
						<section class="content">
                            <div class="panel panel-bd unit-maneg-page">
                                <div class="panel-heading clearfix">
                                   <div class="panel-title pull-left">
                                        <h2>Unit Management</h2>
                                    </div>
                                   
                                </div>
                                
                                <div class="form-common clearfix">
                                	
                                
								  
                                   		<div class="row">
										
										<div class=" col-sm-12 ">
										<br/>
										<ul class="nav nav-tabs">
  
  <li class="active"><a data-toggle="tab" href="#unitmanagement">Unit Management</a></li>
  <% if (user=='OEM'){%>
    <li><a data-toggle="tab" href="#batchmanagement">Batch Management</a></li>
  <%}%>
  
</ul>

<div class="tab-content">
  <div id="unitmanagement" class="tab-pane fade in active">
    <table class="table table-bordered">
                          <thead>
                            <tr>
                              <th width="200" class="text-center">Smartcode Address</th>
                              <th width="150" class="text-center">Batch Number</th>
                              <th width="250" class="text-center">Date of Manufacturing</th>
                              <th width="150" class="text-center">Current Owner</th>
                              <th width="150" class="text-center"></th>
                             
                            </tr>
                          </thead>
                          <tbody>
                            
                              <%for (var i=0;i<units.length;i++){%>
                              <tr class="active">
                              <td align="center"><%=units[i].popcodeData.address.slice(0,32)%></td>
                              <td align="center"><%=units[i].unit.batchNo%></td>
                              <td align="center"><%=units[i].unit.manufactureDate%></td>
                              <td align="center"><%=units[i].ownerInfos[units[i].ownerInfos.length-1].ownerName%></td>
                              <td align="center">
                                  <a href="/<%=user.toLowerCase()%>/getUnitData?address=<%=units[i].popcodeData.address%>" id="<%=units[i].popcodeData.address%>" class="btn btn-warning w-md m-b-5 btn-sm btnRecord">View Record</a>
                                
                                  </td>
                              </tr>
                              <%}%>
                            
                            
                           
                            
                          </tbody>
                        </table>
  </div>
  <div id="batchmanagement" class="tab-pane fade">
    <form class="form-horizontal" action="/oem/generateBatch" method="POST">
  <div class="form-group">
    <label class="control-label col-sm-3" for="skuname">Batch No:-</label>
    <div class="col-sm-9">
      <input type="text" class="form-control" id="skusize" name="BatchNo" value="<%=BatchNo%>" readonly>
    </div>
  </div>
  <div class="form-group">
    <label class="control-label col-sm-3" for="skusize">Batch Quantity</label>
    <div class="col-sm-9"> 
      <input type="text" class="form-control" id="skusize" name="total" required>
    </div>
  </div>
   <div class="form-group">
    <label class="control-label col-sm-3" for="manudate">Manufacturing Date</label>
    <div class="col-sm-9"> 
      <input type="text" class="form-control demo" id="manudate" name="ManufactureDate" required>
    </div>
  </div>
  <div class="form-group">
    <label class="control-label col-sm-3" for="expdate">Expiry Date</label>
    <div class="col-sm-9"> 
      <input type="text" class="form-control demo" id="expdate" name="ExpiryDate" required>
    </div>
  </div>
 <div class="form-group">
  <label for="sel1" class="col-sm-3 control-label ">Product</label>
      <div class="col-sm-9"> 
  <select class="form-control" id="product" required>
      
      <% if (products){%>
        <% for (var i=0;i<products.length;i++){%>
            <option value="<%=i%>"><%=products[i].productName%></option>
        <%}%>
    <%}%>
   
  </select></div>
</div>
<div class="form-group">
  <label for="sel1" class="col-sm-3 control-label ">Sku</label>
      <div class="col-sm-9"> 
          <% if (products){%>
          <input type="hidden" id="totalPro" value="<%=products.length%>"></input>
          <%}%>
  <select class="form-control" id="Skutype" name="skuId" required>
      <option value="">Select Sku</option>
      <% if (products){%>
  <% for (var i=0;i<products.length;i++){%>
  <optgroup class="group<%=i%>">
      
    <% if (products[i].skusNumbers){%>
        <% for (var j=0;j<products[i].skusNumbers.length;j++){%>
        <option><%=products[i].skusNumbers[j]%></option>
    
        <%}%>
        <%}%>
	</optgroup>
    <%}%>
	<%}%>
  </select>
  </div>
</div>

  <div class="form-group"> 
    <div class="col-sm-offset-3 col-sm-9">
      <button type="submit" class="btn btn-success ">Generate</button>
    </div>
  </div>
</form>
 <div class="row">
								   <div class="col-sm-12  ">
								   
 
 <table class="table table-bordered table-responsive">
    <thead>
      <tr>
        
        <th align="center" valign="middle">Batch No</th>
        <th align="center" valign="middle">Quantity</th>
        <th align="center" valign="middle">Manufacturing Date</th>
        <th align="center" valign="middle">Expiry Date</th>
		<th align="center" valign="middle">Product Name</th>
		<th align="center" valign="middle">Sku Id</th>
     
      </tr>
    </thead>
    <tbody>
      <% if (batches){%>
      <% for (var i=batches.length-1;i>=0;i--){%>

      <tr>
      
        <td><%=batches[i].batchNo%></td>
        <td><%=batches[i].batchQuantity%></td>
        <td><%=batches[i].manufactureDate%></td>
        <td><%=batches[i].expiryDate%></td>
		<td><%=batches[i].skuMaster.productMaster.productName%></td>
        <td><%=batches[i].skuMaster.oemSkuId%></td>
      </tr>
      <%}}%>
    </tbody>
  </table>
 
 
 

								   </div>
								   </div>
  </div>
  
</div>
