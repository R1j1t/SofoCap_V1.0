        <script src="/assets/dist/js/dashboard.js" type="text/javascript"></script>
		   <!-- Calender js -->
		  <script src="/assets/dist/js/dcalendar.picker.js" type="text/javascript"></script>
        <!-- End Theme label Script
        =====================================================================-->
        <script>
            $(function () {
                $("#datepicker").datepicker({
                    dateFormat: "dd-mm-yy"
                });
            });
			$(document).ready(function(){
			
            var value= $("#product").val();
			var group=".group"+value;
            var totalPro=$("#totalPro").val();
            
            for (var i=0;i<totalPro;i++){
			    var hidegroup=".group"+i;
                $(hidegroup).hide();
            }
            $("#Skutype").val("");
            $(group).show();
			
			$(".expand").click(function(){
			$(".detail-table").slideUp()
			$(this).parent().parent().children(".detail-table").slideDown();
			});
			
			$("#product").change(function(){
			var value= $("#product").val();
			var group=".group"+value;
            var totalPro=$("#totalPro").val();
        
            for (var i=0;i<totalPro;i++){
			    var hidegroup=".group"+i;
                $(hidegroup).hide();
            }
            $("#Skutype").val("");
            $(group).show();
			
			
			
			});
			$('.demo').dcalendarpicker();
			});
        </script>
    </body>
